package Exercicios;

public class Ex_14 {
	public static void main(String[] args) {
		int idade = 65;
		boolean possuiCarteiraMotorista = false;

		if (idade >= 18 && idade <= 65 || possuiCarteiraMotorista) {
		    System.out.println("Pode dirigir");
		} else {
		    System.out.println("NÃ£o pode dirigir");
		}

	}
}
